# R0, R1, R2 are caller saved
# R3, R4, R5 are callee saved

#integer constants: draw_glyph_start, monitor_structure_start

MOV R5, -(R6)

MOV #0, R0
MOV #0, R1
MOV #07, R2
JSR R5, @#{draw_glyph_start:o}

MOV #1, R0
MOV #0, R1
MOV #04, R2
JSR R5, @#{draw_glyph_start:o}

MOV #2, R0
MOV #0, R1
MOV #013, R2
JSR R5, @#{draw_glyph_start:o}

MOV #3, R0
MOV #0, R1
MOV #017, R2
JSR R5, @#{draw_glyph_start:o}

MOV #{monitor_structure_start:o}, R5
MOV #04, -(R5)
MOV #0, -(R5)
MOV (R6)+, R5
RTS R5
